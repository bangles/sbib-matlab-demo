% try this function with the following inputs
% [par1, par2, par3] = defaultvalue_demo( 1,  2, 3 )
% [par1, par2, par3] = defaultvalue_demo( 1, [], 3 )
% [par1, par2, par3] = defaultvalue_demo( [] )
% [par1, par2, par3] = defaultvalue_demo( [], 1 )
function [par1, par2, par3] = defaultvalue_demo( varargin )
    par1 = defaultvalue( varargin, 1, 'parameter 1 defaulted' );
    par2 = defaultvalue( varargin, 2, nan ); % this needs to have a value
    par3 = defaultvalue( varargin, 3, 'parameter 3 defaulted' );
end
